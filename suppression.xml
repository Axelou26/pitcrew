<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Suppression des avertissements pour les dépendances optionnelles d'esbuild -->
    <suppress>
        <notes>Suppression des avertissements pour les dépendances optionnelles d'esbuild qui sont spécifiques à la plateforme</notes>
        <packageUrl regex="true">^pkg:npm/@esbuild/.*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>

    <!-- Suppression des avertissements pour les dépendances optionnelles de Parcel -->
    <suppress>
        <notes>Suppression des avertissements pour les dépendances optionnelles de Parcel qui sont spécifiques à la plateforme</notes>
        <packageUrl regex="true">^pkg:npm/@parcel/watcher-.*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>

    <!-- Suppression des avertissements pour les dépendances optionnelles de Rollup -->
    <suppress>
        <notes>Suppression des avertissements pour les dépendances optionnelles de Rollup qui sont spécifiques à la plateforme</notes>
        <packageUrl regex="true">^pkg:npm/@rollup/rollup-.*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>

    <!-- Suppression des avertissements pour fsevents (dépendance optionnelle macOS) -->
    <suppress>
        <notes>Suppression des avertissements pour fsevents qui est spécifique à macOS</notes>
        <packageUrl regex="true">^pkg:npm/fsevents@.*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>
</suppressions> 